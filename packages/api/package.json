{
  "name": "@web3-test/api",
  "version": "1.0.0",
  "description": "Backend API for Web3 dApp Testing Platform",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "web3",
    "dapp",
    "testing",
    "api",
    "playwright"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.658.0",
    "@aws-sdk/s3-request-presigner": "^3.658.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/sensible": "^6.0.1",
    "@fastify/swagger": "^9.2.0",
    "@fastify/swagger-ui": "^5.1.0",
    "@fastify/websocket": "^11.0.1",
    "@prisma/client": "^5.22.0",
    "@web3-test/translator": "file:../translator",
    "adm-zip": "^0.5.16",
    "bullmq": "^5.25.5",
    "dockerode": "^4.0.2",
    "ethers": "^6.13.0",
    "fastify": "^5.1.0",
    "ioredis": "^5.4.1",
    "nanoid": "^5.0.8",
    "pino-pretty": "^13.1.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/dockerode": "^3.3.31",
    "@types/node": "^22.9.0",
    "@types/ws": "^8.5.12",
    "prisma": "^5.22.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
